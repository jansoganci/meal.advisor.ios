<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- 
	SECRETS CONFIGURATION TEMPLATE
	
	Copy this file to Secrets.plist and fill in your actual values.
	NEVER commit Secrets.plist to git - it's already in .gitignore.
	
	Setup Instructions:
	1. Copy this file: cp Secrets.example.plist Secrets.plist
	2. Fill in all YOUR_*_HERE placeholders with actual values
	3. Verify Secrets.plist is NOT tracked by git
	-->
	
	<!-- Supabase Configuration -->
	<key>SUPABASE_URL</key>
	<string>YOUR_SUPABASE_URL_HERE</string>
	<key>SUPABASE_ANON_KEY</key>
	<string>YOUR_SUPABASE_ANON_KEY_HERE</string>
	
	<!-- Google Sign-In -->
	<key>GOOGLE_CLIENT_ID</key>
	<string>YOUR_GOOGLE_CLIENT_ID_HERE</string>
	<key>GOOGLE_CLIENT_SECRET</key>
	<string>YOUR_GOOGLE_CLIENT_SECRET_HERE</string>
	
	<!-- Apple Sign-In -->
	<key>APPLE_SERVICE_ID</key>
	<string>YOUR_APPLE_SERVICE_ID_HERE</string>
	<key>APPLE_KEY_ID</key>
	<string>YOUR_APPLE_KEY_ID_HERE</string>
	<key>APPLE_TEAM_ID</key>
	<string>YOUR_APPLE_TEAM_ID_HERE</string>
	<key>APPLE_PRIVATE_KEY</key>
	<string>YOUR_APPLE_PRIVATE_KEY_HERE</string>
	
	<!-- Unsplash API -->
	<key>UNSPLASH_ACCESS_KEY</key>
	<string>YOUR_UNSPLASH_ACCESS_KEY_HERE</string>
	<key>UNSPLASH_SECRET_KEY</key>
	<string>YOUR_UNSPLASH_SECRET_KEY_HERE</string>
</dict>
</plist>
